#include <stdio.h>  
#include <string.h>

int main() {  
    char input[50];  
    scanf("%s", input);

    int i = 0;  
    while (input[i] == '0' && input[i + 1] != '\0') {  
        i++;  
    }

    if (input[i] == '\0') {  
        printf("0\n");  
    } else {  
        char output[50];  
        strncpy(output, input + i, 50);  
        output[49] = '\0';  
        printf("%s\n", output);  
    }

    return 0;  
}
//交换最小值和最大值
#include<stdio.h>
int main()
{
    int n,i,min,max,k=0,x=0,tmp;
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    min=a[0];
    max=a[0];
    for(int j=1;j<n;j++)
    {
        if(a[j]<min)
        { 
            min=a[j];
            k=j;
        } else if(a[j]>max)
        {
            max=a[j];
            x=j;
        }
    }
    tmp = a[0];
    a[0]=min;
    a[k]=tmp;
    if(a[k]==max) x=k;
    tmp = a[n-1];
    a[n-1]=max;
    a[x]=tmp;
    for(int l=0;l<n;l++)
        printf("%d ",a[l]);
    return 0;
}
